generator client {
  provider = "prisma-client-js"
  output   = "../src/generated/prisma"
}

datasource db {
  provider = "postgresql"
  url      = env("DB_URL")
}

model Image {
  id        String   @id @default(cuid())
  name      String
  caption   String
  imageUrl  String
  createdAt DateTime @default(now())
}
